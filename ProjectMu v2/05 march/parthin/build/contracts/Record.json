  "source": "pragma solidity >=0.4.21 <0.7.0;\n\ncontract Record{\n    \n    struct patientRecord{\n        uint patientID;\n        uint doctorID;\n        uint hospitalID;\n        uint blockId;\n    }\n    uint public blockNo;\n    mapping(uint => patientRecord) public allpatientrecords;\n   // function getPatient(uint _hospitalID, uint _patientID, uint _doctorID ) {\n     //   hospitalID = _doctorID;\n       // patientID = _patientID;\n        //doctorID = _doctorID;\n    //}\n    \n    function addPatient(uint _patientID, uint _doctorID,uint _hospitalID) public {\n        blockNo++;\n        allpatientrecords[blockNo] = patientRecord(_patientID, _doctorID, _hospitalID, blockNo);\n    }\n}",
